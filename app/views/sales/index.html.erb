<div class="top_header">
  <div class="header_arrow"><%= link_to "<", homes_path %></div>
  <div class="header_text">Sell Products</div>
  <div class="header_img"><%= image_tag "sell.png"%></div>
  <div class="home-line-break"></div>
</div>

    <% @items.each do |item| %>
      <tr>
        <td><%= item.name %></td>
        <td><%= item.description %></td>

        <td><%= image_tag(item.image.url, :width => 100) %></td>

      </tr>
    <% end %>


<script type="text/javascript">
  setTimeout(function() {
    hideNav();
  },100);
</script>
